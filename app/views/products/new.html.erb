<h1 class="heading">New Product</h1>
<%= form_with(model: @product) do |form| %>
    <%= render 'form', product: @product %>
    <%= form.label "Picture" %>
    <%= form.file_field :picture %>
    <div class="actions">
    <%= form.submit %>
    </div>
    <%= link_to 'Back', products_path %>
<% end %>